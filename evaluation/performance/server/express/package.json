{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run start-tool",
    "start-coverage": "nyc --all --exclude-node-modules=false npm run start-vanilla",
    "start-tool": "sh ./_tool_server.sh",
    "start-vanilla": "node ./bin/www",
    "generate-sbom": "npm sbom --sbom-format cyclonedx >./sbom.json",
    "generate-cbom": "node .cli/src/cli.js --sbom sbom.json --strategy log --build-only --cbom-output cbom.json -- ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  }
}
